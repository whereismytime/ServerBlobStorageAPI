{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false, // Отключаем Windows-аутентификацию
    "anonymousAuthentication": true, // Включаем анонимную аутентификацию
    "iisExpress": {
      "applicationUrl": "http://localhost:58138", // Локальный URL для IIS Express
      "sslPort": 44375 // Порт для SSL
    }
  },
  "profiles": {
    "http": {
      "commandName": "Project", // Запуск проекта напрямую
      "dotnetRunMessages": true, // Включение сообщений о запуске
      "launchBrowser": true, // Автозапуск браузера
      "launchUrl": "swagger", // Переход на Swagger при запуске
      "applicationUrl": "http://localhost:5074", // Локальный HTTP URL
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Установка среды как Development
      }
    },
    "https": {
      "commandName": "Project", // Запуск проекта напрямую
      "dotnetRunMessages": true, // Включение сообщений о запуске
      "launchBrowser": true, // Автозапуск браузера
      "launchUrl": "swagger", // Переход на Swagger при запуске
      "applicationUrl": "https://localhost:7072;http://localhost:5070", // HTTP и HTTPS URL
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Установка среды как Development
      }
    },
    "IIS Express": {
      "commandName": "IISExpress", // Запуск через IIS Express
      "launchBrowser": true, // Автозапуск браузера
      "launchUrl": "swagger", // Переход на Swagger при запуске
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Установка среды как Development
      }
    }
  }
}
